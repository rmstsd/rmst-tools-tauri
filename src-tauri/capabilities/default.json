{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "core:app:allow-default-window-icon",
    "core:window:default",
    "core:window:allow-start-dragging",
    "core:window:allow-hide",
    "store:default",
    "updater:default",
    "os:default",
    "process:default",
    "log:default"
  ]
}
